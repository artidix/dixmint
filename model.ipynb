    "def countPrice(n):\n",
    "  price = 0.1\n",
    "  sum = 0\n",
    "  for i in range(n - 1):\n",
    "    price *= 1.001\n",
    "    sum += price\n",
    "\n",
    "  return (price, sum)"
   ]
